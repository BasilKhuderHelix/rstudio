{
  "id": "tidyverse-dev",
  "version": "0.0.1",
  "name": "Tidyverse Development",
  "description": "A Template to run the latest Tidyverse development environment on Verily Workbench",
  "documentationURL": "https://github.com/yourusername/your-repo",
  "licenseURL": "https://github.com/yourusername/your-repo/blob/master/LICENSE",
  "options": {
    "cloud": {
      "type": "string",
      "description": "VM cloud environment",
      "proposals": ["gcp", "aws"],
      "default": "gcp"
    },
    "login": {
      "type": "string",
      "description": "Whether to log in to workbench CLI",
      "proposals": ["true", "false"],
      "default": "false"
    }
  },
  "platforms": ["Any"]
}
